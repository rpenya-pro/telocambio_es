{
  "name": "@app-landing/react-landing",
  "scripts": {
    "start": "webpack serve --port 9000 --env isLocal",
    "start:all": "concurrently \"yarn run start\" \"yarn run start:app-navigation\" \"yarn run start:app-landing\" \"yarn run start:app-footer\"  \"yarn run start:app-shared\"  \"yarn run start:app-dashboard\"  \"yarn run start:app-profile\"",
    "start:app-navigation": "cd ../app-navigation && yarn run start:standalone",
    "start:app-landing": "cd ../app-landing && yarn run start:standalone",
    "start:app-footer": "cd ../app-footer && yarn run start:standalone",
    "start:app-shared": "cd ../app-shared && yarn run start",
    "start:app-dashboard": "cd ../app-dashboard && yarn run start:standalone",
    "start:app-profile": "cd ../app-profile && yarn run start:standalone",
    "lint": "eslint src --ext js,ts,tsx",
    "test": "cross-env BABEL_ENV=test jest --passWithNoTests",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "prepare": "husky install",
    "build": "concurrently yarn:build:*",
    "build:webpack": "webpack --mode=production",
    "build:types": "tsc"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^20.5.9",
    "@types/testing-library__jest-dom": "^5.14.1",
    "babel-jest": "^27.0.6",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-ts-react-important-stuff": "^3.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "jest-cli": "^27.0.6",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "ts-config-single-spa": "^3.0.0",
    "typescript": "^4.3.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-config-single-spa-react": "^4.0.0",
    "webpack-config-single-spa-react-ts": "^4.0.0",
    "webpack-config-single-spa-ts": "^4.0.0",
    "webpack-dev-server": "^4.0.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/systemjs": "^6.1.1",
    "@types/webpack-env": "^1.16.2",
    "bootstrap": "^5.3.1",
    "js-cookie": "^3.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "single-spa": "^5.9.3",
    "single-spa-layout": "^2.2.0",
    "single-spa-react": "^4.3.1"
  },
  "types": "dist/app-landing-react-landing.d.ts"
}
