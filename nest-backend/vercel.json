{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/user/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        },
        {
            "src": "/threads/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST"
            ]
        },
        {
            "src": "/proposals/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST"
            ]
        },
        {
            "src": "/events/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET"
            ]
        }
    ]
}