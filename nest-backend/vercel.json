{
    "version": 2,
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/user/(.*)",
            "dest": "dist/main.js"
        },
        {
            "src": "/threads/(.*)",
            "dest": "dist/main.js"
        },
        {
            "src": "/proposals/(.*)",
            "dest": "dist/main.js"
        },
        {
            "src": "/events/(.*)",
            "dest": "dist/main.js"
        }
    ]
}